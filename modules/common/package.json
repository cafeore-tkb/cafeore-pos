{
  "name": "@cafeore/common",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsc -d -p tsconfig.json",
    "dl-store": "tsx src/scripts/download-orders.ts",
    "dl-state": "tsx src/scripts/download-master-state.ts",
