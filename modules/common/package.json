  },
  "dependencies": {
    "@remix-run/react": "^2.17.2",
    "firebase": "^12.3.0",
    "lodash": "^4.17.21",
    "swr": "^2.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^4.0.1",
    "@types/lodash": "^4.17.12",
    "@types/node": "^24.5.2",
    "dayjs": "^1.11.13",
    "firebase-tools": "^14.7.0",
    "tsx": "^4.19.2",
    "vitest": "^3.0.5"
  }
}
